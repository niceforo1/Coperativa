	<queryString>
		<![CDATA[SELECT c.ID_CONEXION as conexion,
tcc.DESCRIPCION as categoria,d.CALLE,ISNULL(CONVERT(varchar(5),d.NUMERO),' ')as NUMERO,
ec.DESCRIPCION as estado_conexion,fp.DESCRIPCION as forma_pago,(s.APELLIDO_RAZON_SOCIAL+','+ISNULL(s.NOMBRE_DENOMINACION,''))as nsocio,
tsu.DESCRIPCION as t_suministro,zc.DESCRIPCION as zona,s.ID_SOCIO, COUNT(ps.ID) as periodosimp, cs.SALDO_TOTAL as saldo

FROM CONEXIONES_SALDOS cs
INNER JOIN PERIODOS_SALDOS ps
	ON ps.ID_CONEXION=cs.ID_CONEXION
INNER JOIN CONEXIONES c
	ON c.ID_CONEXION=cs.ID_CONEXION
INNER JOIN SOCIOS s
	ON s.ID_SOCIO=c.ID_SOCIO
INNER JOIN TIPO_DOCUMENTO td
	ON s.ID_TIPO_DOC=td.ID_TIPO_DOC
INNER JOIN  TIPO_SOCIO ts
	ON ts.ID_TIPO_SOCIO=s.ID_TIPO_SOCIO
INNER JOIN ESTADO_SOCIO es
	ON es.ID_ESTADO_SOCIO=s.ID_ESTADO_SOCIO

INNER JOIN ZONAS_CONEXIONES zc
	ON zc.ID_ZONA=c.ID_NRO_ZONA
INNER JOIN TIPO_CATEGORIA_CONEXION tcc
	ON tcc.ID_TIPO_CATEGORIA_CONEXION=c.ID_CATEGORIA_CONEXION
INNER JOIN DOMICILIOS d
	ON d.ID_DOMICILIO=c.ID_DOMICILIO_SERV
INNER JOIN ESTADO_CONEXIONES ec
	ON ec.ID_ESTADO_CONEXION=c.ID_ESTADO_CONEXION
INNER JOIN FORMA_PAGO fp
	ON fp.ID_FORMA_PAGO=c.ID_FORMA_PAGO
INNER JOIN TIPO_SUMINISTRO tsu
	ON tsu.ID_TIPO_SUMINISTRO=c.ID_TIPO_SUMINISTRO

where cs.SALDO_TOTAL<0

GROUP BY c.ID_CONEXION,tcc.DESCRIPCION,d.CALLE,d.NUMERO,ec.DESCRIPCION,fp.DESCRIPCION,s.APELLIDO_RAZON_SOCIAL,s.NOMBRE_DENOMINACION,
tsu.DESCRIPCION,zc.DESCRIPCION,s.ID_SOCIO,cs.SALDO_TOTAL

ORDER BY cs.SALDO_TOTAL]]>
	</queryString>
